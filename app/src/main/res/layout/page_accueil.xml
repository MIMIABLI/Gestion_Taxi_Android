<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="280dp"
        android:layout_height="28dp"
        android:text="Enter your information"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="48dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="273dp"
        android:layout_height="33dp"
        android:text="First name"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="98dp" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="269dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="phone"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="164dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="265dp"
        android:layout_height="35dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="225dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="263dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="277dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="257dp"
        android:layout_height="47dp"
        android:text="Confirm the password"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="357dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="262dp"
        android:layout_height="36dp"
        android:text="Forgot password?"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="430dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="255dp"
        android:layout_height="30dp"
        android:text="Create customer"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="531dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="242dp"
        android:layout_height="31dp"
        android:text="Create Diver Account"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="588dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="369dp"
        android:layout_height="40dp"
        android:text="SEND"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="653dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="275dp"
        android:layout_height="38dp"
        android:text="Login"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="480dp" />